{
  "name": "satoshi-power",
  "version": "1.0.0",
  "description": "Compares the price of a single satoshi with that of the smallest units of each fiat currency.",
  "scripts": {
    "dev": "rollup -w -c 'rollup.config.dev.mjs'",
    "build": "rollup -c 'rollup.config.prod.mjs'",
    "browser-sync": "browser-sync start --config 'bs-config.js'",
    "watch": "concurrently \"yarn dev\" \"yarn browser-sync\""

  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "browser-sync": "^3.0.2",
    "concurrently": "^8.2.2",
    "rollup": "^4.18.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-sass": "^1.13.0"
  }
}
